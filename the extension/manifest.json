{
  "name": "Parsing one",
  "version": "1",
  "author": "Vadagon",
  "manifest_version": 2,
  "permissions": [
    "tabCapture",
    "activeTab",
    "tabs",
    "storage",
    "<all_urls>",
    "webRequest", 
    "webRequestBlocking",
    "notifications"
  ],
  "browser_action": {
    "default_title": "Click to open Popup"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
      "icon.png"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*", "<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
